{
  "compilerOptions": {
	"target": "es2015",
	"noImplicitReturns": true,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	"suppressImplicitAnyIndexErrors": true,
	"strict": true,
	"module": "commonjs",
	"moduleResolution": "node",
	"resolveJsonModule": true,
	"esModuleInterop": true,
	"typeRoots": ["./types", "./node_modules/@types"]
  }
}

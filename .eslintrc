{
  "extends":  [
	"plugin:@typescript-eslint/recommended",
	"airbnb-base"
  ],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "env": {
	"jest": true
  },
  "rules": {
	"no-plusplus": 0,
	"no-continue": 0,
	"import/no-unresolved": 0,
	"no-param-reassign": 0,
	"max-len": ["error", { "code": 120 }]
  }
}
